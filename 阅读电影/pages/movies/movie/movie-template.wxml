<import src="../stars/stars.wxml" />
<template name="movieTemplate">
	<view class="movie-container">
		<image class="movie-img" src="{{coverageurl}}">
		</image>
		<text class="movie-title">{{title}}</text>
		<template is="starsTemplate" data="{{average}}"/>
	</view>
</template>

